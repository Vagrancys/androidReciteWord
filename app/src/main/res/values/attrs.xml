<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleProgressView">
        <attr name="circle_stroke_width" format="dimension"/>
        <attr name="circle_finish_color" format="color"/>
        <attr name="circle_un_finish_color" format="color"/>
        <attr name="circle_arc_angle" format="float"/>
        <attr name="circle_progress" format="float"/>
        <attr name="circle_arc_max" format="integer"/>
    </declare-styleable>

    <declare-styleable name="BookSeekBar">
        <attr name="book_background_width" format="dimension"/>
        <attr name="book_background_height" format="dimension"/>
        <attr name="book_background_color" format="color"/>
    </declare-styleable>
    
    <declare-styleable name="PowerImageView">
        <attr name="auto_play" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="MonthDateView">
        <attr name="baseTextSize" format="dimension"/>
        <attr name="baseTextColor" format="color"/>
        <attr name="rowHeight" format="dimension"/>
    </declare-styleable>
</resources>