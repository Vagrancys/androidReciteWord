<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleProgressView">
        <attr name="circle_stroke_width" format="dimension"/>
        <attr name="circle_finish_color" format="color"/>
        <attr name="circle_un_finish_color" format="color"/>
        <attr name="circle_arc_angle" format="float"/>
        <attr name="circle_progress" format="float"/>
        <attr name="circle_arc_max" format="integer"/>
    </declare-styleable>

    <declare-styleable name="BookSeekBar">
        <attr name="book_background_width" format="dimension"/>
        <attr name="book_background_height" format="dimension"/>
        <attr name="book_background_color" format="color"/>
    </declare-styleable>
    
    <declare-styleable name="PowerImageView">
        <attr name="auto_play" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="MonthDateView">
        <attr name="NowTextColor" format="color"/>
        <attr name="NotTextColor" format="color"/>
        <attr name="DayTextSize" format="dimension"/>
        <attr name="WordTextColor" format="color"/>
        <attr name="WordTextSize" format="dimension"/>
        <attr name="CircleColor" format="color"/>
        <attr name="CircleWidth" format="dimension"/>
        <attr name="SelectTextColor" format="color"/>
        <attr name="ItemHeight" format="dimension"/>
    </declare-styleable>
    
    <declare-styleable name="AvatarCircleView">
        <attr name="border_width" format="dimension"/>
        <attr name="border_color" format="color"/>
        <attr name="border_overlay" format="boolean"/>
    </declare-styleable>
    
    <declare-styleable name="DefaultSeekBar">
        <attr name="circleButtonHeight" format="dimension"/>
        <attr name="circleButtonWidth" format="dimension"/>
        <attr name="tickBarHeight" format="dimension"/>
        <attr name="tickBarColor" format="color"/>
        <attr name="BarClass" format="integer"/>
        <attr name="ButtonTextColor" format="color"/>
        <attr name="ButtonTextSize" format="dimension"/>
        <attr name="TickBarTextColor" format="color"/>
        <attr name="TickBarTextSize" format="dimension"/>
    </declare-styleable>

</resources>





































